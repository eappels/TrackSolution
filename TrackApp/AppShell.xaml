<Shell
    x:Class="TrackApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:TrackApp.Views"
    Title="TrackApp">

    <ShellContent
        Title="Map"
        ContentTemplate="{DataTemplate views:MapView}"
        Route="MapView" />

    <ShellContent
        Title="Dev"
        ContentTemplate="{DataTemplate views:DevView}"
        Route="DevView" />

</Shell>